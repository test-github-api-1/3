              <shortdesc>The text string to match.</shortdesc>
              <datatype>
                <type>String</type>
              </datatype>
            </parameter>
          </parameters>
          <datatype>
            <type href="#/ListItem">ListItem</type>
          </datatype>
        </method>
        <method name="remove">
          <shortdesc>Removes a child item from the list.</shortdesc>
